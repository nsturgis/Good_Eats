<%= form_for @restaurant do |f| %>
<p>
<%= f.label :name %> <br>
<%= f.text_field :name %>
</p>
<p>
<%= f.label :address %> <%= f.text_field :address %><br>
<%= f.label :city %> <%= f.text_field :city %> <br>
<%= f.label :state %> <%= f.text_field :state %> <br>
<%= f.label :zip_code %> <%= f.text_field :zip_code %> <br>
</p>
<p>
<%= f.label :description %><br>
<%= f.text_area :description %>
</p>
<p>
<%= f.label :category %> <br>
<%= f.text_field :category %>
</p>
<%=f.submit %>
<% end %>
